#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/markdown-it@12.3.2/node_modules/markdown-it/bin/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/markdown-it@12.3.2/node_modules/markdown-it/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/markdown-it@12.3.2/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules:/Users/monkey/Dev/sandbox/node_modules:/Users/monkey/Dev/node_modules:/Users/monkey/node_modules:/Users/node_modules:/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/frontmatter-markdown-loader@3.7.0/node_modules/frontmatter-markdown-loader/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/frontmatter-markdown-loader@3.7.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/markdown-it@12.3.2/node_modules/markdown-it/bin/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/markdown-it@12.3.2/node_modules/markdown-it/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/markdown-it@12.3.2/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules:/Users/monkey/Dev/sandbox/node_modules:/Users/monkey/Dev/node_modules:/Users/monkey/node_modules:/Users/node_modules:/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/frontmatter-markdown-loader@3.7.0/node_modules/frontmatter-markdown-loader/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/frontmatter-markdown-loader@3.7.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../markdown-it@12.3.2/node_modules/markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../../../../../markdown-it@12.3.2/node_modules/markdown-it/bin/markdown-it.js" "$@"
fi
