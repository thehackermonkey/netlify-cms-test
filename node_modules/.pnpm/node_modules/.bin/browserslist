#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/browserslist@4.21.0/node_modules/browserslist/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/browserslist@4.21.0/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules:/Users/monkey/Dev/sandbox/node_modules:/Users/monkey/Dev/node_modules:/Users/monkey/node_modules:/Users/node_modules:/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/node_modules/browserslist/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/browserslist@4.21.0/node_modules/browserslist/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/browserslist@4.21.0/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules:/Users/monkey/Dev/sandbox/node_modules:/Users/monkey/Dev/node_modules:/Users/monkey/node_modules:/Users/node_modules:/node_modules:/Users/monkey/Dev/sandbox/netlify-cms-test/node_modules/.pnpm/node_modules/browserslist/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browserslist/cli.js" "$@"
else
  exec node  "$basedir/../browserslist/cli.js" "$@"
fi
